        "data.loc[(data['감정_대분류'] == \"분노\"), '감정_대분류'] = 2  #분노 => 2\n",
        "data.loc[(data['감정_대분류'] == \"슬픔\"), '감정_대분류'] = 3  #슬픔 => 3\n",
        "data.loc[(data['감정_대분류'] == \"neutral\"), '감정_대분류'] = 4  #중립 => 4\n",
